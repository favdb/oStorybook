<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
      "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
          "classpath://org/hibernate/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="storybook.model.hbn.entity">
  <class name="Attribute" table="ATTRIBUTE">
    <id column="ID" name="id" type="java.lang.Long" unsaved-value="null">
      <generator class="increment">
            </generator>
    </id>
    <property column="key" insert="true" length="256" name="key" type="java.lang.String" update="true"/>
    <property column="value" insert="true" length="2048" name="value" type="java.lang.String" update="true"/>
  </class>
</hibernate-mapping>
