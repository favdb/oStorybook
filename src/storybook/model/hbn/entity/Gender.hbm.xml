<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
      "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
          "classpath://org/hibernate/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="storybook.model.hbn.entity">
  <class name="Gender" table="GENDER">
    <id column="ID" name="id" type="java.lang.Long" unsaved-value="null">
      <generator class="increment">
        <!--
                  To add non XDoclet generator parameters, create a file named
                  hibernate-generator-params-Gender.xml
                  containing the additional parameters and place it in your merge dir.
              -->
      </generator>
    </id>
    <property column="name" insert="true" length="256" name="name" type="java.lang.String" update="true"/>
    <property column="childhood" insert="true" name="childhood" not-null="true" type="java.lang.Integer" update="true"/>
    <property column="adolescence" insert="true" name="adolescence" not-null="true" type="java.lang.Integer" update="true"/>
    <property column="adulthood" insert="true" name="adulthood" not-null="true" type="java.lang.Integer" update="true"/>
    <property column="retirement" insert="true" name="retirement" not-null="true" type="java.lang.Integer" update="true"/>
  </class>
</hibernate-mapping>
